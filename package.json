{
  "name": "easywebpack",
  "version": "5.0.0",
  "private": true,
  "devDependencies": {
    "lerna": "^3.22.1",
    "eslint": "^7.30.0",
    "eslint-config-eslint": "^7.0.0"
  },
  "workspaces": [
    "packages/framework/*",
    "packages/plugin/*",
    "packages/cli/*"
  ],
  "scripts": {
    "boot": "lerna workspace",
    "clean": "lerna clean",
    "eslint": "lerna run lint",
    "test": "lerna run test",
    "test:easy": "lerna run test --scope easywebpack",
    "test:vue": "lerna run test --scope easywebpack-vue",
    "test:react": "lerna run test --scope easywebpack-react",
    "test:cli": "lerna run test --scope easywebpack-cli",
    "test:server": "lerna run test --scope webpack-tool",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  }
}
